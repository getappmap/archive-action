{
  "name": "archive-appmap-action",
  "version": "1.0.0",
  "author": "AppLand, Inc",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "compile": "tsc",
    "package": "ncc build -o dist/archive --source-map src/archive.ts && ncc build -o dist/merge --source-map src/merge.ts",
    "build": "yarn compile && yarn package",
    "test": "tsc --noEmit && jest --runInBand",
    "appmap": "appmap-agent-js -- jest --no-cache --runInBand",
    "clean": "rm -rf build dist tmp",
    "format": "prettier --write '**/*.ts'"
  },
  "devDependencies": {
    "@appland/appmap-agent-js": "^13.8.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@vercel/ncc": "^0.36.1",
    "glob": "^9.3.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@actions/artifact": "^1.1.1",
    "@actions/cache": "^3.2.1",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
