{
  "name": "archive-appmap-action",
  "version": "1.0.0",
  "author": "AppLand, Inc",
  "license": "MIT",
  "main": "build/archive.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "jest",
    "appmap": "appmap-agent-js -- jest --no-cache",
    "clean": "rm -rf build dist tmp",
    "format": "prettier --write '**/*.ts'",
    "package": "yarn build && ncc build -o dist/archive --source-map src/archive.ts && ncc build -o dist/merge --source-map src/merge.ts"
  },
  "devDependencies": {
    "@appland/appmap-agent-js": "^13.8.0",
    "@types/argparse": "^2.0.10",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@vercel/ncc": "^0.36.1",
    "glob": "^9.3.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@actions/artifact": "^1.1.1",
    "@actions/cache": "^3.2.1",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@appland/action-utils": "https://github.com/getappmap/action-utils#19e6956b6b078a95154d0ba3fea2c762060b0b45",
    "argparse": "^2.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
